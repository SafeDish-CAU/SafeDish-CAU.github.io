{
  "openapi": "3.0.0",
  "info": {
    "title": "Recommendation API",
    "version": "1.0.0",
    "description": "API 문서: 사용자, 태그, 아이템 CRUD 및 아이템/유저 기반 추천 기능을 제공합니다."
  },
  "paths": {
    "/diner/{diner_id}/menu": {
      "get": {
        "summary": "음식점 메뉴 조회",
        "parameters": [
          {
            "name": "diner_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "조회할 음식점의 UID"
          }
        ],
        "responses": {
          "200": {
            "description": "추천된 아이템 ID 리스트",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DinerMenuResponse"
                }
              }
            }
          },
          "404": {
            "description": "해당 음식점이 존재하지 않음"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DinerMenuResponse": {
        "type": "object"
      }
    }
  }
}